/ {
    chosen {
        #size-cells = <0x00000001>;
        #address-cells = <0x00000001>;
        xen,dom0-bootargs = "###XEN_DOM0_BOOTARGS###";
        xen,xen-bootargs = "###XEN_XEN_BOOTARGS###";

        module@0 {
            reg = <###XEN_DOM0_ADDR### ###XEN_DOM0_SIZE###>;
            compatible = "multiboot,module";
        };
    };
};

